@import "../../app.wxss";
.custom_nav {
  width: 100%;
  background: #fff;
  border-bottom: 1rpx solid #eee;
}

.custom_nav_box {
  position: fixed;
  width: 100%;
  background: #fff;
}
.custom_nav_bar{
  position: relative;
}
.custom_nav_box .nav_title {
  /*color: #000;*/
  text-align: center;
  position: absolute;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 34rpx;
  color: #2c2b2b;
  font-weight: 500;
}

.custom_nav_box .custom_nav_icon {
  position: absolute;
  border: .5rpx solid rgba(0, 0, 0, .1);
  border-radius: 50%;
  display: flex;
  /*padding: 0 30rpx;*/
}
.custom_nav_icon .gobank {
  background: url('https://img.intech.gdinsight.com/byd/a20200403han/icon-back.png') no-repeat center center;
  background-size: 18%;
  padding:0 15px;
  margin: 4px 0;
}
.custom_nav_icon .home {
  background: url('https://img.intech.gdinsight.com/byd/a20200403han/icon-gohome.png') no-repeat center center;
  background-size:35%;
  padding:0 15px;
  margin: 4px 0;
  border-left: 1px solid rgba(0, 0, 0, .1)
}
